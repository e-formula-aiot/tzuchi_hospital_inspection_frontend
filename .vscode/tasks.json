{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "nuxtjs: start",
      "type": "npm",
      "script": "dev",
      "isBackground": true,
      "problemMatcher": {
        "base": "$tsc-watch",
        "background": {
          "activeOnStart": true,
          "beginsPattern": "Preparing project for development",
          "endsPattern": "Listening on"
        }
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
